{
  "version": 2,
  "functions": {
    "api/stanza/stanza.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    {
      "src": "generatesuggestions.js",
      "use": "@vercel/node"
},
{
  "src": "api/stanza/stanza.py",
  "use": "@vercel/python"
}
  ],
  "routes": [
    { "src": "/api/v2/check", "dest": "/index.js" },
    {
      "src": "/api/v2/geminiapi",
      "dest": "/generatesuggestions.js"
},
{
  "src": "/v2/check/stanza/",
  "dest": "/api/stanza/stanza.py"
}
  ]
}
